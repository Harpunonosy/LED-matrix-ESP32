{
  "expo": {
    "entryPoint": "app/App.tsx",
    "name": "Blinkster",
    "slug": "Blinkster",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "blinkster",
    "android": {
      "package": "com.anonymous.Blinkster"
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.Blinkster"
    },
    "plugins": [
      "expo-dev-client"
    ],
    "extra": {
      "eas": {
        "projectId": "62a7bbbc-2029-499e-9629-6b44958b4b1c"
      }
    }
  }
}
